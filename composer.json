{
    "name": "waseem/laravel-data-encryption",
    "description": "Automatically encryption and decryption data overriding by using getAttribute an setAttribute methods of Eloquent Model.",
    "keywords": ["encryption", "laravel", "laravel 10.x","decryption", "eloquent", "data model"],
    "license": "MIT",
    "version": "1.0.1",
    "autoload": {
        "psr-4": {
            "Waseem\\Encipher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Waseem\\Encipher\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Waseem",
            "email": "waseem.ahmad970@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-openssl": "*",
        "illuminate/database": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0",
        "mockery/mockery": "1.3.1",
        "laravel/legacy-factories": "^1.1"
    },
    "scripts": {
      "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waseem\\Encipher\\EncipherServiceProvider"
            ],
            "dont-discover": [
                "*"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
