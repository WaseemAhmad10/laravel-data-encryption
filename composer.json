{
    "name": "waseem/laravel-data-encryption",
    "description": "Automatically encryption and decryption data overriding by using getAttribute an setAttribute methods of Eloquent Model.",
    "keywords": ["encryption", "laravel", "laravel 10.x","decryption", "eloquent", "data","model"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Waseem\\Encipher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Waseem\\Encipher\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Waseem Ahmad",
            "email": "waseem.ahmad970@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-openssl": "*",
        "illuminate/database": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0",
        "mockery/mockery": "1.3.1",
        "laravel/legacy-factories": "^1.1"
    },
    "minimum-stability": "stable",
    "scripts": {
      "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waseem\\Encipher\\EncryptServiceProvider"
            ],
            "dont-discover": [
                "*"
            ]
        }
    }
}
